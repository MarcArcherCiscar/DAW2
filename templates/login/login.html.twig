<body>

<!-- header-wrap -->
<div id="header-wrap">
    <header>

        <hgroup>
            <h1><a href="127.0.0.1:8000/">DAW2</a></h1>
            <h3>Desarrollo Web en Entorno de Servidor</h3>
        </hgroup>

        <nav>
            <ul>
                <li><a href="127.0.0.1:8000/">Home</a></li>
                <li><a href="http://127.0.0.1:8000/servicios">Servicios</a></li>
                <li><a href="proyectos.html.twig">Proyectos</a></li>
                <li><a href="http://127.0.0.1:8000/nosotros">Sobre Nosotros</a></li>
                <li><a href="http://127.0.0.1:8000/contacta">Contacta</a></li>
                <li><a href="http://127.0.0.1:8000/login">Login</a></li>
                <li><p>{{Nom_User}}</p></li>
            </ul>
        </nav>
    </header>
</div>

<!-- content-wrap -->
<div class="content-wrap">

      <!-- login -->
      <section id="login" class="clearfix">

            <h1>Login</h1>

            <div class="primary">
                {% if EstaLogejat == false %}
                <p class="intro">Introduce tus datos de usuario para iniciar la sesión.</p>
                {% endif %}
                {% if EstaLogejat %}
                <p class="intro">Usuario AAAA conectado.</p>
                {% endif %}
                <form method="post" action="#" id="loginform">
                
                    <div>
                        <p>Credenciales</p>
                    </div>
                
                    <div>
                        <label>Usuario <span class="required">*</span></label>
                        <input name="user" type="text" id="user" value="" />
                    </div>
                
                    <div>
                        <label>Contraseña <span class="required">*</span></label>
                        <input name="pasword" type="password" id="password" value="" />
                    </div>     
                    
                    <div>
                        <input type="submit" value="Entrar" class="button">
                        <input type="reset" value="Reset" class="button">
                    </div>
                
                </form>
            </div>



      </section>

</div>

<!-- footer -->
<footer>
    <div class="footer-content">
        <ul class="footer-menu">
